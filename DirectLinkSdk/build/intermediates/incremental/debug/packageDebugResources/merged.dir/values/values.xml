<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:ns1="http://schemas.android.com/tools">
    <color name="black">#000000</color>
    <color name="colorAccent">#3253FA</color>
    <color name="colorPrimary">#3253FA</color>
    <color name="colorPrimaryDark">#3253FA</color>
    <color name="white">#ffffff</color>
    <string name="app_name">SDK New</string>
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
        <item name="android:windowFullscreen">true</item>
    </style>
    <style name="CustomDialogAD" parent="Theme.AppCompat.Dialog">
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <style name="transparent_dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">true</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
    </style>
    <declare-styleable name="TemplateView" ns1:ignore="ResourceName">
        <attr format="reference" name="gnt_template_type"/>
        <attr format="integer" name="gnt_template_min_height"/>
    </declare-styleable>
</resources>